{
  "name": "ceiboo/ddd",
  "license": "MIT",
  "type": "project",
  "description": "Ejemplo de Monorepositorio desarrollado por Ceiboo",
  "require": {
    "php": ">7.4",

    "ext-apcu": "*",
    "ext-json": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",

    "symfony/framework-bundle": "^4.3",
    "symfony/dotenv": "^4.3",
    "symfony/yaml": "^4.3",
    "lambdish/phunctional": "^1.0",

    "ramsey/uuid": "^3.8",

    "doctrine/dbal": "^2.9",
    "doctrine/orm": "^2.6",
    "symfony/messenger": "^5.4"
  },
  "require-dev": {
    "ext-xdebug": "*",

    "roave/security-advisories": "dev-master",

    "behat/behat": "^3.5",
    "behat/mink-extension": "^2.3",
    "behat/mink-browserkit-driver": "^1.3",
    "friends-of-behat/symfony-extension": "^2.0",

    "phpunit/phpunit": "^8.2",

    "fzaninotto/faker": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Ceiboo\\Apps\\Api\\Mid10\\": "apps/api/mid10/src",
      "Ceiboo\\Apps\\Backend\\Auth\\": "apps/backend/auth/src",
      "Ceiboo\\": ["boundedcontexts"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ceiboo\\Tests\\": ["tests/boundedcontexts"]
    }
  }
}
